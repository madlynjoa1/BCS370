#include<iostream>
#include<string>

using namespace std;

template<classType>
Type larger(Type x, Type y);

int main()
{
  cout <<"Larger of 5 and 6 : " << larger(5,6)<< endl;;
  cout << "Larger of A and B :" << larger(A,B)<< endl;
  cout << "Larger of 5.6 and 3.1 : "<< larger(5.6, 3.1)<< endl;
  return 0;
}

template <class Type>
Type larger(Type x , Type y)
{
  if( x>= y)
    return x;
    else return y;
}
